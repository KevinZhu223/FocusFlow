# FocusFlow Environment Variables
# Copy this file to .env and update with your values

# PostgreSQL Database URL
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/focusflow

# JWT Secret Key (change this in production!)
JWT_SECRET=focusflow-dev-secret-change-in-production

# OpenAI API Key for LLM features (activity parsing & coach insights)
OPENAI_API_KEY=your_openai_api_key_here
